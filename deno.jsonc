{
  "name": "@lenskit/typeline",
  "version": "0.1.0",
  "imports": { "@std/path": "jsr:@std/path@^1.0.6", "zod": "npm:zod@^3.23" },
  "exports": {
    ".": "./mod.ts"
  },
  "lint": {
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "prefer-const"
      ],
      "include": [
        "no-undef",
        "no-const-assign"
      ]
    }
  },
  "compilerOptions": {
    "lib": [
      "ES2020",
      "dom"
    ]
  },
  "fmt": {
    "lineWidth": 100,
    "indentWidth": 2
  }
}
